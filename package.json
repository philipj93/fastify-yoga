{
  "name": "fastify-yoga",
  "description": "Simple GraphQL server built with GraphQL Yoga and Fastify",
  "private": true,
  "version": "0.1.0",
  "packageManager": "yarn@4.0.0-rc.5",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prisma": {
    "seed": "esno -r tsconfig-paths/register scripts/seed.ts"
  },
  "scripts": {
    "watch": "tsup-node src/index.ts --format cjs,esm --watch",
    "build": "tsup-node src/index.ts --format cjs,esm --dts-resolve --sourcemap",
    "dev": "esno scripts/start.ts",
    "start": "node dist/index.js",
    "reset": "esno scripts/reset.ts",
    "test": "vitest",
    "typecheck": "tsc --pretty --noEmit",
    "lint": "TIMING=1 eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "is-ci || husky install",
    "commit": "gacp"
  },
  "dependencies": {
    "@envelop/depth-limit": "1.5.3",
    "@envelop/response-cache": "2.3.1",
    "@graphql-yoga/node": "2.5.0",
    "@pothos/core": "3.10.1",
    "@pothos/plugin-prisma": "3.6.1",
    "@pothos/plugin-relay": "3.10.2",
    "@prisma/client": "3.14.0",
    "env-schema": "4.0.0",
    "fastify": "3.29.0",
    "graphql": "16.5.0",
    "graphql-scalars": "1.17.0",
    "pino": "7.11.0",
    "pino-pretty": "7.6.1",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@commitlint/lint": "16.2.4",
    "@types/eslint": "8.4.2",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "@vitest/ui": "0.12.3",
    "c8": "7.11.2",
    "dotenv": "16.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "esno": "0.14.1",
    "execa": "6.1.0",
    "gacp": "3.0.1",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "prettier-plugin-prisma": "3.13.0",
    "pretty-quick": "3.1.3",
    "prisma": "3.14.0",
    "prisma-repl": "1.3.0",
    "supertest": "6.2.3",
    "tasuku": "2.0.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "tsup": "5.12.7",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.12.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint --fix"
    ]
  },
  "gacp": {
    "emoji": "emoji"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
